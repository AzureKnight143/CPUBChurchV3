{
  "name": "cpubchurchv3",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "bs": "browser-sync start --config wp-content/themes/cpubchurchv3/src/build/browser-sync.config.js",
    "css": "npm-run-all css-compile css-postcss css-minify",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css --quiet wp-content/themes/cpubchurchv3/src/sass/child-theme.scss:wp-content/themes/cpubchurchv3/css/child-theme.css node_modules/understrap/src/sass/custom-editor-style.scss:wp-content/themes/cpubchurchv3/css/custom-editor-style.css",
    "css-minify": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output wp-content/themes/cpubchurchv3/css/ --batch --batch-suffix \".min\" \"wp-content/themes/cpubchurchv3/css/*.css\" \"!wp-content/themes/cpubchurchv3/css/*.min.css\" \"!wp-content/themes/cpubchurchv3/css/*rtl*.css\"",
    "css-postcss": "postcss --config wp-content/themes/cpubchurchv3/src/build/postcss.config.js --replace \"wp-content/themes/cpubchurchv3/css/*.css\" \"!wp-content/themes/cpubchurchv3/css/*.rtl*.css\" \"!wp-content/themes/cpubchurchv3/css/*.min.css\"",
    "js": "npm-run-all js-compile js-minify",
    "js-compile": "rollup --config wp-content/themes/cpubchurchv3/src/build/rollup.config.js --sourcemap",
    "js-minify": "terser --config-file wp-content/themes/cpubchurchv3/src/build/terser.config.json --output wp-content/themes/cpubchurchv3/js/child-theme.min.js wp-content/themes/cpubchurchv3/js/child-theme.js",
    "watch": "npm-run-all --parallel watch-run-*",
    "watch-bs": "npm-run-all --parallel bs watch-run-*",
    "watch-run-css": "nodemon --watch wp-content/themes/cpubchurchv3/src/sass/ --ext scss --exec \"npm-run-all css\"",
    "watch-run-js": "nodemon --watch wp-content/themes/cpubchurchv3/src/js/ --ext js --exec \"npm-run-all js\"",
    "dist-build": "node wp-content/themes/cpubchurchv3/src/build/dist-build.js",
    "dist-clean": "node wp-content/themes/cpubchurchv3/src/build/dist-clean.js",
    "docker-up": "docker-compose up",
    "docker-down": "docker-compose down"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@popperjs/core": "^2.11.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "autoprefixer": "^10.4.0",
    "bootstrap": "^5.1.3",
    "browser-sync": "^2.27.7",
    "browserslist": "^4.18.1",
    "caniuse-lite": "^1.0.30001284",
    "clean-css-cli": "^5.4.2",
    "del": "^6.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "postcss-understrap-palette-generator": "git+https://github.com/understrap/postcss-understrap-palette-generator.git",
    "rollup": "^2.60.2",
    "sass": "^1.44.0",
    "terser": "^5.10.0",
    "understrap": "github:understrap/understrap#develop"
  }
}
